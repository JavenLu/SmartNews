<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <javen.example.com.commonlibrary.custom_view.search_view.SearchView
        android:id="@+id/search_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:searchBlockColor="@color/white"
        app:searchBlockHeight="120"
        app:textColor="@color/colorAccent"
        app:textHint="@string/search_view_hint_text"
        app:textHintColor="@color/colorText"
        app:textHintSize="@dimen/x30"
        app:textSize="@dimen/x8" />
</LinearLayout>